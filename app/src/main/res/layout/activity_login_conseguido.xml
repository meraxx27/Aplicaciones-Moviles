<?xml version="1.0" encoding="utf-8"?>
<RelativeLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    android:background="@drawable/background"
    tools:context=".LoginConseguido">

    <TextView
        android:id="@+id/txtname"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_below="@id/Notify"
        android:layout_marginLeft="70dp"
        android:layout_marginTop="50dp"
        android:text="TextView"
        android:textColor="@color/white"
        android:textSize="25dp"/>

    <TextView
        android:id="@+id/txtlastname"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_below="@id/Notify"
        android:layout_marginLeft="230dp"
        android:layout_marginTop="50dp"
        android:text="TextView"
        android:textColor="@color/white"
        android:textSize="25dp"/> />

    <Button
        android:id="@+id/Volver"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_alignParentRight="true"
        android:text="Cerrar Sesion"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="parent"
        app:layout_constraintVertical_bias="0.834"
        tools:ignore="MissingConstraints" />

    <Button
        android:id="@+id/Notify"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_centerHorizontal="true"
        android:layout_marginTop="200dp"
        android:text="Notificacion"
        tools:ignore="MissingConstraints" />

    <TextView
        android:id="@+id/txtemail"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_below="@id/Notify"
        android:layout_centerHorizontal="true"
        android:layout_marginTop="120dp"
        android:text="TextView"
        android:textColor="@color/white"
        android:textSize="25dp" /> />

    <TextView
        android:id="@+id/txtnac"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_below="@id/Notify"
        android:layout_centerHorizontal="true"
        android:layout_marginTop="170dp"
        android:text="TextView"
        android:textColor="@color/white"
        android:textSize="25dp" /> />

    <TextView
        android:id="@+id/txtfecha"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_below="@id/Notify"
        android:layout_centerHorizontal="true"
        android:layout_marginTop="230dp"
        android:text="TextView"
        android:textColor="@color/white"
        android:textSize="25dp" />

    <Button
        android:id="@+id/Edit"
        android:layout_width="163dp"
        android:layout_height="69dp"
        android:layout_below="@id/txtfecha"
        android:layout_marginTop="30dp"
        android:text="Editar" />

    <Button
        android:id="@+id/delete"
        android:layout_width="163dp"
        android:layout_height="69dp"
        android:layout_below="@id/txtfecha"
        android:layout_alignParentRight="true"
        android:layout_marginStart="90dp"
        android:layout_marginTop="30dp"
        android:layout_marginRight="0dp"
        android:layout_toEndOf="@+id/Edit"
        android:text="Eliminar" />

    <ImageView
        android:id="@+id/porfilpicture"
        android:layout_width="wrap_content"
        android:layout_height="174dp"
        app:srcCompat="@drawable/descarga" />

    <Button
        android:id="@+id/cambiarimagen"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_below="@id/txtfecha"
        android:layout_marginTop="120dp"
        android:layout_centerHorizontal="true"
        android:background="@drawable/boton_modificacion"
        android:text="Cambiar Foto de Perfil" />

    <Button
        android:id="@+id/btnmapa"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:text="Mapa"
        android:background="@drawable/boton_modificacion"
        android:layout_alignParentRight="true"
        android:layout_below="@id/Volver"
        android:layout_marginTop="50dp"/>

</RelativeLayout>